{
  "name": "root",
  "private": true,
  "packageManager": "pnpm@9.1.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint .",
    "build": "nx run-many --target=build",
    "clean": "rimraf packages/*/*/node_modules && rimraf node_modules",
    "test": "nx run-many --target=test",
    "test-cov": "nyc --reporter=lcov --reporter=text-summary nx run-many --target=test --skipNxCache"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@nx/jest": "19.0.3",
    "jest": "^29.4.1",
    "nx": "19.0.3",
    "nyc": "^15.1.0",
    "rimraf": "^1.0.3"
  }
}
